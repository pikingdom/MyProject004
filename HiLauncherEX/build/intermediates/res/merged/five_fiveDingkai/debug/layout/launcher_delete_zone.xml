<?xml version="1.0" encoding="utf-8"?>
<com.nd.hilauncherdev.launcher.screens.DeleteZone
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/launcher_delete_zone" android:layout_width="fill_parent"
		android:layout_height="@dimen/delete_zone_size" 
		android:layout_gravity="top|center_horizontal" android:orientation="horizontal"
		android:visibility="invisible">
		<com.nd.hilauncherdev.launcher.DeleteZoneTextView
			android:id="@+id/im_hint"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" />
		<com.nd.hilauncherdev.launcher.DeleteZoneTextView
			android:id="@+id/im_uninstallIm" 
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" />
			
		<com.nd.hilauncherdev.launcher.DeleteZoneTextView
			android:id="@+id/im_delete"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" />
	</com.nd.hilauncherdev.launcher.screens.DeleteZone>
