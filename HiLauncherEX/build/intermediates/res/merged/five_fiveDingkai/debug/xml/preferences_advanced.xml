        <CheckBoxPreference
            android:defaultValue="false"
            android:key="settings_communicate_location"
            android:layout="@layout/preference_normal_layout"
            android:title="@string/settings_communicate_location"
            android:widgetLayout="@layout/preferences_checkbox" />
