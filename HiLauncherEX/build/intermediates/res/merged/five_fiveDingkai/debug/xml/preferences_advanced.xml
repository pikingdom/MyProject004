            android:layout="@layout/preference_normal_layout"
            android:title="@string/settings_communicate_boot_time"
            android:widgetLayout="@layout/preferences_checkbox" />
        <!-- 隐藏应用 -->
        <Preference
                android:key="key_hide_app_encrypt_switch"
