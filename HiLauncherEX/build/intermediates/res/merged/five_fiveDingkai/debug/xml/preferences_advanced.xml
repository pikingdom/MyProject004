                android:key="settings_advanced_clear_apk"
                android:layout="@layout/preference_normal_layout"
                android:summary="@string/settings_advanced_clear_apk_tips"
                android:title="@string/settings_advanced_clear_apk"
                android:widgetLayout="@layout/preferences_checkbox" />
        <!-- 开启桌面菜单通知中心 -->
        <CheckBoxPreference
