            android:widgetLayout="@layout/preferences_listpreferences_more_btn" />
        <!-- 浏览器默认主页 -->
        <Preference
            android:key="settings_advanced_browser_default_home"
            android:layout="@layout/preference_normal_layout"
            android:title="@string/settings_advanced_browser_default_home"
            android:widgetLayout="@layout/preferences_listpreferences_more_btn" />
        <!-- 开启桌面"逗你玩"功能 -->
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="settings_advanced_bazinga"
            android:layout="@layout/preference_normal_layout"
            android:title="@string/settings_advanced_bazinga"
            android:widgetLayout="@layout/preferences_checkbox" />
        <!-- 图标拦截 -->
        <CheckBoxPreference
                android:defaultValue="true"
                android:key="settings_advanced_shortcut_icon_intercept"
                android:layout="@layout/preference_normal_layout"
                android:title="@string/settings_advanced_shortcut_icon_intercept"
                android:summary="@string/settings_advanced_shortcut_icon_intercept_detail"
                android:widgetLayout="@layout/preferences_checkbox" />
         <!-- 安装包清理 -->
        <CheckBoxPreference
