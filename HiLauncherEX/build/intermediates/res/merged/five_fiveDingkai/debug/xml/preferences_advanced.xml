    <com.nd.hilauncherdev.settings.PromptPreferenceCategory
        android:height="50dp"
        android:key="settings_communicate"
        android:title="@string/settings_communicate_category_comm" >

        <!-- 消息提示 -->
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="settings_message_tips"
            android:layout="@layout/preference_normal_layout"
            android:summary="@string/settings_message_tips_summary"
            android:title="@string/settings_message_tips"
            android:widgetLayout="@layout/preferences_checkbox" />
