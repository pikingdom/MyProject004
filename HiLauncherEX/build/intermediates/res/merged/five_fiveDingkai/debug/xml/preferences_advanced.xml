    <com.nd.hilauncherdev.settings.PromptPreferenceCategory
        android:height="50dp"
        android:title="@string/settings_communicate_category_battery" 
        android:key="settings_communicate_category_battery">

        <!-- 充电完成提醒 -->
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="settings_communicate_battery_full_notification"
            android:layout="@layout/preference_normal_layout"
            android:summary="@string/mybattery_soft_setting_full_notification_desc"
            android:title="@string/mybattery_soft_setting_full_notification_title"
            android:widgetLayout="@layout/preferences_checkbox" />
        <!-- 低电量提醒 -->
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="settings_communicate_battery_lowhit"
            android:layout="@layout/preference_normal_layout"
            android:summary="@string/mybattery_soft_setting_lowhit_desc"
            android:title="@string/mybattery_soft_setting_lowhit_title"
            android:widgetLayout="@layout/preferences_checkbox" />
