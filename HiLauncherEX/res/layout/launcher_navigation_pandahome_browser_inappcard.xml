<?xml version="1.0" encoding="utf-8"?>
<!-- 桌面内置浏览器inapp搜索卡片 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <LinearLayout
        android:id="@+id/launcher_navigation_pandahome_inapp_inappcard"
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:orientation="vertical"/>
	<LinearLayout
	    android:id="@+id/launcher_navigation_pandahome_inapp_inappcard_button"
	    android:layout_width="match_parent"
    	android:layout_height="40dp"
    	android:background="@drawable/common_btn_blue"
    	android:gravity="center"
    	android:layout_margin="10dp">
	    <TextView
	        android:id="@+id/launcher_navigation_pandahome_inapp_inappcard_text"
	        android:layout_width="wrap_content"
    		android:layout_height="wrap_content"
	        android:textColor="@color/white"
	        android:textSize="15sp"
	        android:text="@string/launcher_navigation_pandahome_inapp_inappcard_text"
	        />
	</LinearLayout>
</LinearLayout>
