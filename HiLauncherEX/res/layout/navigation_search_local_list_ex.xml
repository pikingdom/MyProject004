<?xml version="1.0" encoding="utf-8"?>
<!-- 搜索结果List -->
<com.nd.hilauncherdev.drawer.view.searchbox.SearchResultExpandableListView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
	android:id="@+id/relationlistView">
    
	<ExpandableListView android:id="@+id/schListView"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
		android:layout_marginLeft="@dimen/search_result_list_margin"
		android:layout_marginRight="@dimen/search_result_list_margin"
		android:listSelector="#00000000"
	    android:cacheColorHint="#00000000"
	    android:dividerHeight="0dp"
	    android:divider="@null"
	    android:groupIndicator="@null" 
	    android:scrollbars="none"/>

</com.nd.hilauncherdev.drawer.view.searchbox.SearchResultExpandableListView>