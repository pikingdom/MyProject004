<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/myphone_bg_color">
    <com.nd.hilauncherdev.shop.shop3.customview.ThemeHeaderView
        android:id="@+id/mHeadView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />
    <!-- 意见反馈 -->
    <com.nd.hilauncherdev.menu.personal.widget.PersonalCenterSettingsItem
        android:id="@+id/personal_center_other_tools_feedback"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <!--实用小工具-->
    <com.nd.hilauncherdev.menu.personal.widget.PersonalCenterSettingsItem
        android:id="@+id/personal_center_other_tools_my_assistant"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <!--消息中心-->
    <com.nd.hilauncherdev.menu.personal.widget.PersonalCenterSettingsItem
        android:id="@+id/personal_center_other_tools_message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>
